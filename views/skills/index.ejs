<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Express Skills</title>
</head>
<body>
  <a href="/">Home</a>
  <h1>All Skills</h1>
  <ul>
    <% skills.forEach(function(sk) { %>
      <li>
        <a href="/skills/<%= sk.id %>"><%= sk.skill %></a>
          - 
        <%= sk.done ? 'done' : 'not done' %>
      </li>
    <% }); %>
  </ul>
</body>


</html>